        "button.secondaryBackground": "#533483",
        "button.secondaryForeground": "#ffffff",
        "button.secondaryHoverBackground": "#442a6e",
        
        "badge.background": "#e94560",
        "badge.foreground": "#ffffff",
        
        "scrollbar.shadow": "#00000030",
        "scrollbarSlider.background": "#0f346080",
        "scrollbarSlider.hoverBackground": "#0f3460",
        "scrollbarSlider.activeBackground": "#e94560",
        
        "progressBar.background": "#e94560",
        
        "minimap.background": "#1a1a2e",
        "minimap.selectionHighlight": "#e9456080",
        "minimap.findMatchHighlight": "#00d9ff80",
        
        "peekView.border": "#e94560",
        "peekViewEditor.background": "#16213e",
        "peekViewEditor.matchHighlightBackground": "#e9456040",
