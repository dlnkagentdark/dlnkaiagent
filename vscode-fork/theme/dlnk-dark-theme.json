        "menu.background": "#16213e",
        "menu.foreground": "#ffffff",
        "menu.selectionBackground": "#0f3460",
        "menu.selectionForeground": "#ffffff",
        "menu.separatorBackground": "#2d2d44",
        "menubar.selectionBackground": "#0f3460",
        "menubar.selectionForeground": "#ffffff",
        
        "settings.headerForeground": "#ffffff",
        "settings.modifiedItemIndicator": "#e94560",
        "settings.dropdownBackground": "#16213e",
        "settings.dropdownForeground": "#ffffff",
        "settings.dropdownBorder": "#2d2d44",
        "settings.checkboxBackground": "#16213e",
        "settings.checkboxForeground": "#ffffff",
        "settings.checkboxBorder": "#2d2d44",
        "settings.textInputBackground": "#16213e",
        "settings.textInputForeground": "#ffffff",
        "settings.textInputBorder": "#2d2d44",
        "settings.numberInputBackground": "#16213e",
        "settings.numberInputForeground": "#ffffff",
        "settings.numberInputBorder": "#2d2d44"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#6c757d",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "Number",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#d19a66"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#e94560"
            }
        },
        {
            "name": "Function",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "Class",
            "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
            "settings": {
                "foreground": "#ffc107"
            }
        },
        {
            "name": "Variable",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Parameter",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "Constant",
            "scope": ["constant", "constant.language"],
            "settings": {
                "foreground": "#d19a66"
            }
        },
        {
            "name": "Operator",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#e94560"
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#a0a0a0"
            }
        },
        {
            "name": "Tag",
            "scope": ["entity.name.tag", "meta.tag"],
            "settings": {
                "foreground": "#e94560"
            }
        },
        {
            "name": "Attribute",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#ffc107"
            }
        },
        {
            "name": "Property",
            "scope": ["support.type.property-name", "meta.object-literal.key"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "Type",
            "scope": ["support.type", "entity.name.type"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "Decorator",
            "scope": ["meta.decorator", "punctuation.decorator"],
            "settings": {
                "foreground": "#533483"
            }
        },
        {
            "name": "Import",
            "scope": ["keyword.control.import", "keyword.control.from"],
            "settings": {
                "foreground": "#e94560"
            }
        },
        {
            "name": "Module",
            "scope": ["entity.name.import", "entity.name.package"],
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "Escape",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#ff4757"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": ["markup.heading", "entity.name.section"],
            "settings": {
                "foreground": "#e94560",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": ["markup.bold"],
            "settings": {
                "foreground": "#ffc107",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": ["markup.italic"],
            "settings": {
                "foreground": "#e5c07b",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Link",
            "scope": ["markup.underline.link"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "Markdown Code",
            "scope": ["markup.inline.raw", "markup.fenced_code"],
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "JSON Key",
            "scope": ["support.type.property-name.json"],
            "settings": {
                "foreground": "#00d9ff"
            }
        },
        {
            "name": "JSON String",
            "scope": ["string.quoted.double.json"],
            "settings": {
                "foreground": "#98c379"
            }
        }
    ]
}
