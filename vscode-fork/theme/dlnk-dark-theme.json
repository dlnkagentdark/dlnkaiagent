        "terminal.ansiBrightWhite": "#ffffff",
        "terminalCursor.foreground": "#e94560",
        
        "input.background": "#16213e",
        "input.foreground": "#ffffff",
        "input.border": "#2d2d44",
        "input.placeholderForeground": "#6c757d",
        "inputOption.activeBackground": "#0f3460",
        "inputOption.activeBorder": "#e94560",
        "inputValidation.errorBackground": "#ff475720",
        "inputValidation.errorBorder": "#ff4757",
        "inputValidation.warningBackground": "#ffc10720",
        "inputValidation.warningBorder": "#ffc107",
        "inputValidation.infoBackground": "#00d9ff20",
        "inputValidation.infoBorder": "#00d9ff",
        
        "dropdown.background": "#16213e",
        "dropdown.foreground": "#ffffff",
        "dropdown.border": "#2d2d44",
        "dropdown.listBackground": "#1a1a2e",
        
        "button.background": "#e94560",
        "button.foreground": "#ffffff",
